# Corrections

# using :q when exiting terminal
alias :q='exit'
alias whouseport='sudo netstat -tulpn | grep'

# youtube-dl extract mp3
alias youtube-mp3='youtube-dl --extract-audio --audio-format mp3'
alias nosleepnet="ping -i 0.2 `netstat -nr | grep -m 1 '^default' | awk '{print $2;}'`"

# python
alias p='python'
alias venv='source venv/bin/activate'

if [ -f ~/.aliases.local ]; then
    source ~/.aliases.local
fi


